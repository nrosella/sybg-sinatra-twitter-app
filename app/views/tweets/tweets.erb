<h1>Timeline</h1>

<div class="col-md-6">
  <h2>How may hours would you like to go back?</h2>
  <form action="/tweets" method="POST">
    <div class="form-group">
      Number of hours
      <input type="number" id="Name" name="hour" class="form-control">
    </div>
    <input type="submit" class="btn btn-default" value="Submit">
</form>

<br>

<% @timeline.each do |tweet| %>
<!--   Remember our @tweets is an array of arrays -->
  <p> <%= tweet[0] %>: <%= tweet[1] %> </p>
<% end %>

 <% @other.each do |t| %>
   <tr>
     <td><img src="<%= t %>"></td>
   </tr>
	 <% end %>